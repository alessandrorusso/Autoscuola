<?xml version="1.0" encoding="utf-8"?>
<form>
	<fields name="profileautoscuola">
		<fieldset name="profileautoscuola"
			label="PLG_USER_PROFILEAUTOSCUOLA_SLIDER_LABEL"
		>
			<field
				name="address"
				type="text"
				id="address"
				required="true"
				description="PLG_USER_PROFILEAUTOSCUOLA_FIELD_ADDRESS_DESC"
				filter="string"
				label="PLG_USER_PROFILEAUTOSCUOLA_FIELD_ADDRESS_LABEL"
				size="30"
			/>			

			<field
				name="city"
				type="text"
				id="city"
				required="true"
				description="PLG_USER_PROFILEAUTOSCUOLA_FIELD_CITY_DESC"
				filter="string"
				label="PLG_USER_PROFILEAUTOSCUOLA_FIELD_CITY_LABEL"
				size="30"
			/>

			<field
				name="region"
				type="text"
				id="region"
				required="true"
				description="PLG_USER_PROFILEAUTOSCUOLA_FIELD_REGION_DESC"
				filter="string"
				label="PLG_USER_PROFILEAUTOSCUOLA_FIELD_REGION_LABEL"
				size="30"
			/>			

			<field
				name="postal_code"
				type="text"
				id="postal_code"
				required="true"
				description="PLG_USER_PROFILEAUTOSCUOLA_FIELD_POSTAL_CODE_DESC"
				filter="string"
				label="PLG_USER_PROFILEAUTOSCUOLA_FIELD_POSTAL_CODE_LABEL"
				size="30"
			/>

			<field
				name="phone"
				type="tel"
				id="phone"
				required="true"
				description="PLG_USER_PROFILEAUTOSCUOLA_FIELD_PHONE_DESC"
				filter="string"
				label="PLG_USER_PROFILEAUTOSCUOLA_FIELD_PHONE_LABEL"
				size="30"
			/>
			
			<field
				name="office"
				type="text"
				id="office"
				required="true"
				description="PLG_USER_PROFILEAUTOSCUOLA_FIELD_OFFICE_DESC"
				filter="string"
				label="PLG_USER_PROFILEAUTOSCUOLA_FIELD_OFFICE_LABEL"
				size="30"			
				
			>
			<option value="Colleferro">colleferro</option>
			<option value="Palestrina">palestrina</option>
			<option value="Paliano">paliano</option>
			</field>	
                        
                        <field
				name="license"
				type="text"
				id="license"
				required="true"
				description="PLG_USER_PROFILEAUTOSCUOLA_FIELD_LICENSE_DESC"
				filter="string"
				label="PLG_USER_PROFILEAUTOSCUOLA_FIELD_LICENSE_LABEL"
				size="30"			
				
			>
			<option value="A">A</option>
			<option value="B">B</option>
			<option value="C">C</option>
                        <option value="D">D</option>
                        <option value="E">E</option>
                        <option value="AM">AM</option>
                        <option value="Nautica">Nautica</option>
                        <option value="Speciali">Speciali</option>
			</field>	
			
			<field
				name="car"
				type="text"
				id="car"
				description="PLG_USER_PROFILEAUTOSCUOLA_FIELD_CAR_DESC"
				filter="string"
				label="PLG_USER_PROFILEAUTOSCUOLA_FIELD_CAR_LABEL"
				size="30"
			>                         
				<option value="Auto1">auto1</option>
				<option value="Auto2">auto2</option>
                                <!--option value="Auto3">auto3</option-->
			</field>
                        
                        <field
				name="bike"
				type="text"
				id="bike"
				description="PLG_USER_PROFILEAUTOSCUOLA_FIELD_BIKE_DESC"
				filter="string"
				label="PLG_USER_PROFILEAUTOSCUOLA_FIELD_BIKE_LABEL"
				size="30"
			>			
				<option value="Moto1">moto1</option>				
			</field>
                        
                        <field
				name="truck"
				type="text"
				id="truck"
				description="PLG_USER_PROFILEAUTOSCUOLA_FIELD_TRUCK_DESC"
				filter="string"
				label="PLG_USER_PROFILEAUTOSCUOLA_FIELD_TRUCK_LABEL"
				size="30"
			>                                
				<option value="Camion1">camion1</option>
			</field>
		</fieldset>
	</fields>
</form>
